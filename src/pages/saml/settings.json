{
    "strict": true,
    "debug": true,
    "sp": {
        "entityId": "https://login.uiowa.edu/metadata/",
        "assertionConsumerService": {
            "url": "https://login.uiowa.edu/?acs",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
        },
        "singleLogoutService": {
            "url": "https://login.uiowa.edu/?sls",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
        },
        "NameIDFormat": "urn:oasis:names:tc:SAML:1.1:nameid-format:unspecified",
        "x509cert": "",
        "privateKey": ""
    },
    "idp": {
        "entityId": "https://login.uiowa.edu/saml/metadata/<onelogin_connector_id>",
        "singleSignOnService": {
            "url": "https://login.uiowa.edu/uip/login.page?service=http://localhost:8501/Login",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
        },
        "singleLogoutService": {
            "url": "https://login.uiowa.edu/trust/saml2/http-redirect/slo/<onelogin_connector_id>",
            "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
        },
        "x509cert": "<onelogin_connector_cert>"
    }
}
